/*  Loader  Start */

.wrapper {
	padding-left: 1em;
	padding-right: 1em;
	margin: auto;
	display: block;
	width: 180px;
}

.cssload-loader {
	width: 45px;
	height: 45px;
	border-radius: 50%;
	margin: 3em;
	display: inline-block;
	position: relative;
	vertical-align: middle;
}

.cssload-loader {
	width: 45px;
	height: 45px;
	border-radius: 50%;
	margin: 3em;
	display: inline-block;
	position: relative;
	vertical-align: middle;
}
.cssload-loader,
.cssload-loader:before,
.cssload-loader:after {
	animation: 2.95s infinite ease-in-out;
		-o-animation: 2.95s infinite ease-in-out;
		-ms-animation: 2.95s infinite ease-in-out;
		-webkit-animation: 2.95s infinite ease-in-out;
		-moz-animation: 2.95s infinite ease-in-out;
}
.cssload-loader:before,
.cssload-loader:after {
	width: 100%; 
	height: 100%;
	border-radius: 50%;
	position: absolute;
	top: 0;
	left: 0;
}

.cssload-loader:before,
.cssload-loader:after {
		content: "";
}

.cssload-loader {
		background-color: white;
		animation: cssload-flower-fan 1.18s infinite linear;
		-o-animation: cssload-flower-fan 1.18s infinite linear;
		-ms-animation: cssload-flower-fan 1.18s infinite linear;
		-webkit-animation: cssload-flower-fan 1.18s infinite linear;
		-moz-animation: cssload-flower-fan 1.18s infinite linear;
}
.cssload-loader:before{
	content: "";
	width: 80%;
	height: 80%;
	background-color: white;
	top: 10%;
	left: 10%;
	box-shadow: 4px -3px 0 rgba(255,100,100,0.7),
				4px 4px 0 rgba(100,255,100,0.7),
				-3px 4px 0 rgba(100,100,255,0.7),
				-4px -4px 0 rgba(240,240,120,0.7);
		-o-box-shadow: 4px -3px 0 rgba(255,100,100,0.7),
				4px 4px 0 rgba(100,255,100,0.7),
				-3px 4px 0 rgba(100,100,255,0.7),
				-4px -4px 0 rgba(240,240,120,0.7);
		-ms-box-shadow: 4px -3px 0 rgba(255,100,100,0.7),
				4px 4px 0 rgba(100,255,100,0.7),
				-3px 4px 0 rgba(100,100,255,0.7),
				-4px -4px 0 rgba(240,240,120,0.7);
		-webkit-box-shadow: 4px -3px 0 rgba(255,100,100,0.7),
				4px 4px 0 rgba(100,255,100,0.7),
				-3px 4px 0 rgba(100,100,255,0.7),
				-4px -4px 0 rgba(240,240,120,0.7);
		-moz-box-shadow: 4px -3px 0 rgba(255,100,100,0.7),
				4px 4px 0 rgba(100,255,100,0.7),
				-3px 4px 0 rgba(100,100,255,0.7),
				-4px -4px 0 rgba(240,240,120,0.7);
}
.cssload-loader:after{
	content: "";
	border: 0px solid white;
	z-index: 2;
	top: -3px;
	left: -3px;
}




@keyframes cssload-flower-fan {
	0%	 { transform: rotate(0deg); }
	100% { transform: rotate(360deg); }
}

@-o-keyframes cssload-flower-fan {
	0%	 { -o-transform: rotate(0deg); }
	100% { -o-transform: rotate(360deg); }
}

@-ms-keyframes cssload-flower-fan {
	0%	 { -ms-transform: rotate(0deg); }
	100% { -ms-transform: rotate(360deg); }
}

@-webkit-keyframes cssload-flower-fan {
	0%	 { -webkit-transform: rotate(0deg); }
	100% { -webkit-transform: rotate(360deg); }
}

@-moz-keyframes cssload-flower-fan {
	0%	 { -moz-transform: rotate(0deg); }
	100% { -moz-transform: rotate(360deg); }
}

/* Loader end*/